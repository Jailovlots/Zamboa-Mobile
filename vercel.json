{
    "version": 2,
    "buildCommand": "npm run build",
    "builds": [
        {
            "src": "api/index.ts",
            "use": "@vercel/node"
        }
    ],
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/api/index.ts"
        }
    ],
    "functions": {
        "api/index.ts": {
            "includeFiles": "static-build/**"
        }
    }
}